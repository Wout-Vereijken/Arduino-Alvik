from arduino import *
from arduino_alvik import ArduinoAlvik

alvik = ArduinoAlvik()

def setup():
  alvik.begin()
  delay(1000)

def loop():
  # twerking ig?
  alvik.set_wheels_speed(20,-20)
  delay(1000)
  alvik.set_wheels_speed(-20,20)
  delay(1000)

def cleanup():
  alvik.stop()

start(setup, loop, cleanup)